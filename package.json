{
  "name": "stencil-tour-of-heroes",
  "private": true,
  "version": "0.0.1",
  "description": "Stencil App Starter",
  "scripts": {
    "build": "stencil build",
    "start": "npm-run-all -p start:*",
    "start:api": "json-server api/db.json --watch --port 4000 --routes api/routes.json",
    "start:client": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^1.12.2",
    "@stencil/router": "^1.0.1",
    "@stencil/store": "^1.3.0",
    "@types/jest": "24.9.1",
    "@types/puppeteer": "2.0.1",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "json-server": "^0.16.1",
    "npm-run-all": "^4.1.5",
    "puppeteer": "2.1.1"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.2"
  }
}
